    {
      "parameters": {
        "operation": "executeQuery",
        "query": "=INSERT INTO mitarbeiter (\n  vorname,\n  nachname,\n  email,\n  abteilung,\n  position,\n  startdatum,\n  status,\n  erstellt_am\n) VALUES (\n  '{{ $json.vorname }}',\n  '{{ $json.nachname }}',\n  '{{ $json.email }}',\n  '{{ $json.abteilung }}',\n  '{{ $json.position }}',\n  '{{ $json.startdatum }}',\n  'onboarding',\n  NOW()\n)\nRETURNING *",
        "options": {}
      },
      "id": "e8135860-3cd3-421b-9164-60c17626740f",
      "name": "Mitarbeiter in DB anlegen",
      "type": "n8n-nodes-base.postgres",
